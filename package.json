  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "generate-ssl": "sh ./scripts/generate-ssl-certificate.sh",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:styles": "stylelint '**/*.{scss,css}'",
    "lint:styles:fix": "stylelint '**/*.{scss,css}' --fix",
    "start": "FASTBOOT_DISABLED=true ember serve NODE_EXTRA_CA_CERTS=$(mkcert -CAROOT)/rootCA.pem ",
    "start:prod": "ember serve -prod NODE_EXTRA_CA_CERTS=$(mkcert -CAROOT)/rootCA.pem ",
    "start:prod:fastboot-disabled": "FASTBOOT_DISABLED=true ember serve -prod NODE_EXTRA_CA_CERTS=$(mkcert -CAROOT)/rootCA.pem ",
    "start:ssl-disabled": "ember serve --ssl=false",
    "start:use-mirage": "FASTBOOT_DISABLED=true USE_MIRAGE=true ember serve NODE_EXTRA_CA_CERTS=$(mkcert -CAROOT)/rootCA.pem ",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember exam",
    "test:ember:coverage": "COVERAGE=true ember exam",
    "test:ember:serve": "ember exam --serve",
    "prepare": "husky install"
  },
